{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "TAVILY_API_KEY": "@tavily_api_key",
    "FIRECRAWL_API_KEY": "@firecrawl_api_key"
  },
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  }
} 